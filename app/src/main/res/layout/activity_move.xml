            android:text="@string/This_move_activity"
            android:gravity="center"
            android:textColor="@color/black"
            android:textStyle="italic"
            android:textSize="30dp"/>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>